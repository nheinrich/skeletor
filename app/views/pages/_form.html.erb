<%= form_for @page do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :project_id %><br />
    <%= f.text_field :project_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :markup %><br />
    <%= f.text_area :markup %>
  </p>
  <p>
    <%= f.label :styles %><br />
    <%= f.text_area :styles %>
  </p>
  <p><%= f.submit %></p>
<% end %>
